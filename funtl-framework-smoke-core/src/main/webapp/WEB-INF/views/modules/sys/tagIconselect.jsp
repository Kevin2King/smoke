<script type="text/javascript">
	$(function(){
		$(".fa-item").click(function(){
			$(".fa-item").removeClass("label-info");
			$(this).addClass("label-info");
			$("#iconInput").val($(this).attr("class").replace("fa-item ", "").replace(" label-info", ""));
		});

		$(".fa-item").dblclick(function(){
			$(".modal-footer").find("button[value='ok']").trigger("click");
		});

		// 选择图标
		$("#iconButton").click(function() {
			var icon = $("#iconInput").val();
			$("#iconIcon").attr("class", icon);
			$("#iconIconLabel").text(icon);
			$("#icon").val(icon);
		});

		// 清除图标
		$("#iconButtonClear").click(function() {
			$("#iconIcon").attr("class", "");
			$("#iconIconLabel").text("无");
			$("#icon").val("");
		});
	});
</script>